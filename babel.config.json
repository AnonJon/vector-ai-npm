{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "client": "./src/client",
          "documents": "./src/documents",
          "methods": "./src/methods",
          "migrations": "./src/migrations",
          "routes": "./src/routes"
        }
      }
    ]
  ]
}
